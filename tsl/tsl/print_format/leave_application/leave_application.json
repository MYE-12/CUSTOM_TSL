 "html": "<meta name=\"pdfkit-orientation\" content=\"portrait\" />\n<meta name=\"page-size\" content ='A4'/>\n    <header id='header-html'>\n\n\n<table width = 100% style=\"table-align:center;\">\n<tr>\n    {% set cyrix_employee = frappe.db.get_value(\"Employee\",doc.employee,['cyrix_employee_']) %}\n    \n    {% if cyrix_employee == 1 %}\n    <td width= 30% rowspan = 2 colspan = 3 ><img width = 200px height = 70px src= \"/files/cyrix.jpeg\"></td>\n    {% else %}\n    <td width= 30% rowspan = 2 colspan = 3 ><img width = 200px height = 70px src= \"/files/TSL Logo.png\"></td>\n    {% endif %}\n    <td colspan = 6 style=\"text-align:center;font-size:16px;line-height:11px\" ><br><b>LEAVE APPLICATION</b></td>\n</tr>\n<tr>\n    <td colspan = 6 style=\"text-align:center;font-size:16px;line-height:11px\" ><br><b>{{doc.company}}</b></td>\n</tr>\n</table>\n\n</header>\n<hr>\n    <!-- Leave Application Details Table -->\n    <table border = 1px width = 100%>\n        <tr>\n            <td colspan = 2 style = \"border-color:#0066AD;background-color:#0066AD;text-align:center;color:white;font-weight:bold\">Employee Details</td>\n        </tr>\n        <tr>\n            <td width = 26%><strong>Employee</strong></td>\n            <td>{{ doc.employee_name }}</td>\n        </tr>\n        <tr>\n            <td><strong>Employee ID</strong></td>\n            <td>{{ doc.employee }}</td>\n        </tr>\n        <tr>\n            <td><strong>Designation</strong></td>\n            <td>{{ doc.designation or '-' }}</td>\n        </tr>\n        <tr>\n            <td><strong>Department</strong></td>\n            <td>{{ doc.department or '-' }}</td>\n        </tr>\n        <tr>\n            <td><strong>Branch</strong></td>\n            <td>{{ doc.branch or '-' }}</td>\n        </tr>\n        <tr>\n            <td><strong>Location</strong></td>\n            <td>{{ doc.location or '-' }}</td>\n        </tr>\n    </table>\n    \n    <br>\n    <br>\n    <table border = 1px width = 100%>\n        <tr>\n            <td colspan = 4 style = \"border-color:#0066AD;background-color:#0066AD;text-align:center;color:white;font-weight:bold\">Leave Details</td>\n        </tr>\n        <tr>\n            <td><strong>Leave Type</strong></td>\n            <td colspan = 3>{{ doc.leave_type  or '-'}}</td>\n        </tr>\n        <tr>\n            <td style = \"width:20%\"><strong>From Date</strong></td>\n            <td style = \"width:30%\">{{ frappe.utils.formatdate(doc.from_date)  or '-'}}</td>\n            <td style = \"width:20%\"><strong>To Date</strong></td>\n            <td style = \"width:30%\">{{ frappe.utils.formatdate(doc.to_date)  or '-'}}</td>\n        </tr>\n        <tr>\n            <td><strong>No. of Days</strong></td>\n            <td>{{ doc.no_of_days  or '-'}}</td>\n            <td><strong>Leave Days</strong></td>\n            <td>{{ doc.leave_days  or '-'}}</td>\n        </tr>\n        <tr>\n            <td><strong>Reason</strong></td>\n            <td colspan = 3>{{ doc.description  or '-'}}</td>\n        </tr>\n    </table>\n    {% if doc.lop_start_date %}\n    <br>\n    <br>\n    <table border = 1px width = 100%>\n        <tr>\n            <td colspan = 6 style = \"border-color:#0066AD;background-color:#0066AD;text-align:center;color:white;font-weight:bold\">Unpaid Days Details</td>\n        </tr>\n        <tr>\n            <td><strong>Unpaid Start Date</strong></td>\n            <td>{{ frappe.utils.formatdate(doc.lop_start_date)  or '-'}}</td>\n            <td><strong>Unpaid End Date</strong></td>\n            <td>{{ frappe.utils.formatdate(doc.lop_end_date)  or '-'}}</td>\n            <td><strong>Unpaid Days</strong></td>\n            <td>{{ doc.lop_days  or '-'}}</td>\n        </tr>\n        \n    </table>\n    {% endif %}\n    {% if doc.from_destination %}\n    <br>\n    <br>\n    <table border = 1px width = 100%>\n        <tr>\n            <td colspan = 4 style = \"border-color:#0066AD;background-color:#0066AD;text-align:center;color:white;font-weight:bold\">Travel Details</td>\n        </tr>\n        <tr>\n            <td><strong>From Destination</strong></td>\n            <td>{{ doc.from_destination  or '-'}}</td>\n            <td><strong>To Destination</strong></td>\n            <td>{{ doc.to_destination  or '-'}}</td>\n        </tr>\n        <tr>\n            <td><strong>Travel Date</strong></td>\n            <td>{{ frappe.utils.formatdate(doc.travel_date)  or '-'}}</td>\n            <td><strong>Return Date</strong></td>\n            <td>{{ frappe.utils.formatdate(doc.return_date)  or '-'}}</td>\n        </tr>\n        \n    </table>\n    {% endif %}\n    {% if doc.name1 %}\n    <br>\n    <br>\n    <table border = 1px width = 100%>\n        <tr>\n            <td colspan = 2 style = \"border-color:#0066AD;background-color:#0066AD;text-align:center;color:white;font-weight:bold\">Contact Details</td>\n        </tr>\n        <tr>\n            <td><strong>Name</strong></td>\n            <td>{{ doc.name1  or '-'}}</td>\n        </tr>\n        <tr>\n            <td><strong>Mobile No</strong></td>\n            <td>{{ doc.mobile_no  or '-'}}</td>\n        </tr>\n        <tr>\n            <td><strong>Relation</strong></td>\n            <td>{{ doc.relation  or '-'}}</td>\n        </tr>\n    </table>\n    {% endif %}\n\n\n<footer id=\"footer-html\" class=\"footer-html\">\n    {% set footer = frappe.db.get_value(\"Letter Head\",\"TSL New\",['footer']) %}\n                {{ footer }}\n                </footer>\n     \n    ",
