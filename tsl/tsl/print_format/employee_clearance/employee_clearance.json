 "css": "\n.print-format{\n    /*font-size:20px;*/\n    font-family: 'Calibri';\n    /*margin-top:5px;*/\n    /*margin-left:100px;*/\n    /*margin-right:100px;*/\n    /*margin-bottom:5px;*/\n}    \n\ntable {\n  /*width:80%;*/\n  /*margin-top:0px;*/\n  /*margin-left:25px;*/\n  /*margin-right:25px;*/\n  /*margin-bottom:1px;*/\n}\n\n.column {\n  float: left;\n  width: 50%;\n  padding: 15px;\n}\n\n\n\n",
 "custom_format": 1,
 "default_print_language": "en",
